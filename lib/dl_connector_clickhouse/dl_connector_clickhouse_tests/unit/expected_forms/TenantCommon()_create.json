        {
            "type": "cache_ttl_sec",
            "name": "cache_ttl_sec"
        },
        {
            "items": [
                {
                    "id": "label",
                    "text": "Raw SQL level",
                    "align": "start",
                    "helpText": "- We do not recommend combining subqueries in datasets with RLS or restrictions on viewing tables.\n- SQL queries from charts enter the connection directly and do not take into account the RLS configured in the datasets."
                },
                {
                    "id": "radio_group",
                    "name": "raw_sql_level",
                    "options": [
                        {
                            "content": {
                                "text": "Forbid"
                            },
                            "value": "off"
                        },
                        {
                            "content": {
                                "text": "Allow subselects in datasets",
                                "hintText": "This option allows you to describe dataset sources using SQL queries"
                            },
                            "value": "subselect"
                        },
                        {
                            "content": {
                                "text": "Allow subselects in datasets and queries from charts",
                                "hintText": "This option allows you to describe dataset sources using SQL queries and create QL charts"
                            },
                            "value": "dashsql"
                        }
                    ],
                    "defaultValue": "off",
                    "controlProps": {}
                }
            ]
        },
        {
            "type": "collapse",
            "name": "advanced_settings",
            "inner": true,
            "text": "Advanced connection settings"
        },
        {
            "items": [
                {
                    "id": "label",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    },
                    "text": "TLS",
                    "helpText": "When the option is enabled, the HTTPS protocol is used when interacting with the database, and when disabled \u2014 HTTP"
                },
                {
                    "id": "radio_button",
                    "name": "secure",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    },
                    "defaultValue": "on",
                    "options": [
                        {
                            "text": "Off",
                            "value": "off"
                        },
                        {
                            "text": "On",
                            "value": "on"
                        }
                    ]
                }
            ]
        },
        {
            "items": [
                {
                    "id": "label",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    },
                    "text": "CA Certificate"
                },
                {
                    "id": "file-input",
                    "name": "ssl_ca",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    }
                }
            ]
        },
        {
            "items": [
                {
                    "id": "label",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    },
                    "text": "Disable data export",
                    "helpText": "Disabling data export will hide the data export button in the charts, though you will still be able to extract data from charts and take screenshots."
                },
                {
                    "id": "radio_button",
                    "name": "data_export_forbidden",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    },
                    "defaultValue": "off",
                    "options": [
                        {
                            "text": "Off",
                            "value": "off"
                        },
                        {
                            "text": "On",
                            "value": "on"
                        }
                    ]
                }
            ]
        },
        {
            "items": [
                {
                    "id": "label",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    },
                    "text": "Readonly",
                    "align": "start"
                },
                {
                    "id": "select",
                    "name": "readonly",
                    "displayConditions": {
                        "advanced_settings": "opened"
                    },
                    "defaultValue": "2",
                    "width": "s",
                    "availableValues": [
                        {
                            "content": "0",
                            "value": "0"
                        },
                        {
                            "content": "1",
                            "value": "1"
                        },
                        {
                            "content": "2",
                            "value": "2"
                        }
                    ],
                    "controlProps": {
                        "showSearch": false
                    }
                }
            ]
        }
