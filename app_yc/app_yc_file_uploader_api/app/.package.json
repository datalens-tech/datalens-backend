{
    "meta": {
        "name": "bi-file-uploader-yc",
        "maintainer": "DataLens <robot-datalens@yandex-team.ru>",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "datalens/backend/app_yc/app_yc_file_uploader_api/app"
        ],
        "build_type": "release"
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "datalens/backend/app_yc/app_yc_file_uploader_api/app/bi_file_uploader_app"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app_yc/app_yc_file_uploader_api/etc"
            },
            "destination": {
                "path": "/etc"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app_yc/app_yc_file_uploader_api/app",
                "files": [
                    "Dockerfile",
                    "gunicorn_logging.ini"
                ]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
