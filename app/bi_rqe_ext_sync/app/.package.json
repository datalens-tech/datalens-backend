{
    "meta": {
        "name": "bi-rqe-ext-sync",
        "maintainer": "DataLens <robot-datalens@yandex-team.ru>",
        "version": "{revision}"
    },

    "build": {
        "targets": ["datalens/backend/app/bi_rqe_ext_sync/app"],
        "build_type": "release"
    },

    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "datalens/backend/app/bi_rqe_ext_sync/app/bi_rqe_ext_sync"
            },
            "destination": {
                "path": "/"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_rqe_ext_sync/etc"
            },
            "destination": {
                "path": "/etc"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_rqe_ext_sync/uwsgi"
            },
            "destination": {
                "path": "/uwsgi"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_rqe_ext_sync/app",
                "files": ["Dockerfile"]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
