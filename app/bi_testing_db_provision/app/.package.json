{
    "meta": {
        "name": "sklad-fixtur",
        "maintainer": "DataLens <robot-datalens@yandex-team.ru>",
        "version": "{revision}"
    },

    "build": {
        "targets": ["datalens/backend/app/bi_testing_db_provision/app"],
        "build_type": "release"
    },

    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "datalens/backend/app/bi_testing_db_provision/app/bi_testing_db_provision_app"
            },
            "destination": {
                "path": "/"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_testing_db_provision/etc"
            },
            "destination": {
                "path": "/etc"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_testing_db_provision/app",
                "files": ["Dockerfile", "gunicorn_logging.ini"]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
