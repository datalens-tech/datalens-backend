{
    "meta": {
        "name": "dls",
        "maintainer": "DataLens <robot-datalens@yandex-team.ru>",
        "version": "{revision}"
    },

    "build": {
        "targets": ["datalens/backend/app/dls/app"],
        "build_type": "release"},

    "data": [
        {"source": {"type": "BUILD_OUTPUT", "path": "datalens/backend/app/dls/app/bidls_app"},
         "destination": {"path": "/"}},

        {"source": {"type": "ARCADIA", "path": "datalens/backend/app/dls/docker"},
         "destination": {"path": "/docker"}},

        {"source": {"type": "ARCADIA", "path": "datalens/backend/app/dls/etc"},
         "destination": {"path": "/etc"}},

        {"source": {
            "type": "ARCADIA",
            "path": "datalens/backend/app/dls/app",
            "files": ["Dockerfile"]},
         "destination": {"path": "/"}}

    ]
}
