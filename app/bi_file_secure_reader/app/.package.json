{
    "meta": {
        "name": "bi-file-secure-reader",
        "maintainer": "DataLens <robot-datalens@yandex-team.ru>",
        "version": "{revision}"
    },

    "build": {
        "targets": ["datalens/backend/app/bi_file_secure_reader/app"],
        "build_type": "release"
    },

    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "datalens/backend/app/bi_file_secure_reader/app/bi_file_secure_reader_app"
            },
            "destination": {
                "path": "/"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_file_secure_reader/etc"
            },
            "destination": {
                "path": "/etc"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_file_secure_reader/app",
                "files": ["Dockerfile"]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
