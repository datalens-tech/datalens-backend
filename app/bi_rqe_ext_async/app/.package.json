{
    "meta": {
        "name": "bi-rqe-ext-async",
        "maintainer": "DataLens <robot-datalens@yandex-team.ru>",
        "version": "{revision}"
    },

    "build": {
        "targets": ["datalens/backend/app/bi_rqe_ext_async/app"],
        "build_type": "release"
    },

    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "datalens/backend/app/bi_rqe_ext_async/app/bi_rqe_ext_async"
            },
            "destination": {
                "path": "/"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_rqe_ext_async/etc"
            },
            "destination": {
                "path": "/etc"
            }
        },

        {
            "source": {
                "type": "ARCADIA",
                "path": "datalens/backend/app/bi_rqe_ext_async/app",
                "files": ["Dockerfile"]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
